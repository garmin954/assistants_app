{
  "$schema": "https://schema.tauri.app/config/1.0.6-rc",
  "productName": "UFACTORY Assistant",
  "version": "1.0.6",
  "identifier": "com.arm-assistants.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1530",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": {
          "allow": [
            "**"
          ]
        }
      }
    },
    "withGlobalTauri": false
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "targets": "all",
    "windows": {
      "wix": {
        "language": "zh-CN"
      }
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [
      "./binaries/xarmparam"
    ]
  },
  "plugins": {
    "updater": {
      "windows": {
        "installMode": "passive"
      },
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDlDRTQxQzQwRDkwNTMyODUKUldTRk1nWFpRQnprbkxxdk4reEI0WVN6SEpaRVlKQm04KzZ6WHFjZURkUVF4YWNWYTdES1QrZ3EK",
      "endpoints": [
        "http://192.168.1.19/releases/xarm/assistant/releases.json"
      ],
      "dangerousInsecureTransportProtocol": true
    }
  }
}